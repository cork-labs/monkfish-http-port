{
  "name": "@cork-labs/monkfish-port-http",
  "version": "0.1.3-pre",
  "description": "HTTP Port adapter for Node.js framework Monkfish",
  "license": "MIT",
  "homepage": "https://github.com/cork-labs/monkfish-port-http",
  "author": "Andre Torgal <andre.torgal@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/cork-labs/monkfish-port-http.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "_mocha --require ts-node/register --watch --watch-extensions ts --recursive 'src/**/*.spec.*' 'test/**/*.spec.*'",
    "lint": "tslint -p .",
    "lint-fix": "tslint -p . --fix",
    "test": "nyc _mocha --require ts-node/register --recursive 'src/**/*.spec.*' 'test/**/*.spec.*'",
    "test-u": "nyc _mocha --require ts-node/register --recursive 'src/**/*.spec.*'",
    "test-i": "nyc _mocha --require ts-node/register --recursive 'test/**/*.spec.*'",
    "coverage": "node_modules/.bin/http-server coverage/",
    "clean": "rm -rf .nyc_output coverage build",
    "dist": "rm -rf dist; tsc -b src/",
    "build": "npm run clean; npm run lint && npm run test && npm run dist",
    "pub": "git push && npm run build && npm-bump patch"
  },
  "dependencies": {
    "@cork-labs/class-timing": "^0.5.0",
    "@cork-labs/interface-emitter": "^0.3.1",
    "@cork-labs/monkfish": "^0.4.0",
    "@cork-labs/monkfish-logger": "^0.6.4",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "on-headers": "^1.0.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.120",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/sinon-chai": "^3.2.2",
    "@types/uuid": "^3.4.4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "http-server": "^0.11.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^7.2.2",
    "sinon-chai": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2"
  }
}
